{
  "name": "phaser3-protobuf-atlas",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run generate-proto & tsc",
    "generate-proto": "pbjs src/protobuf/atlas.proto -l -t static-module --no-create --no-encode --no-verify --no-convert --no-delimited --no-service -w src/protobuf/wrapper --es6 -o src/protobuf/index.js",
    "postgenerate-proto": "pbts src/protobuf/index.js -m -o src/protobuf/index.d.ts"
  },
  "exports": {
    "./encode": {
      "require": "./dist/encode/index.js",
      "import": "./dist/encode/index.js",
      "types": "./dist/encode/index.d.ts"
    },
    "./atlas-loader": {
      "require": "./dist/atlas-loader/foo.js",
      "import": "./dist/atlas-loader/foo.js",
      "types": "./dist/atlas-loader/foo.d.ts"
    },
    "./multi-atlas-loader": {
      "require": "./dist/multi-atlas-loader/bar.js",
      "import": "./dist/multi-atlas-loader/bar.js",
      "types": "./dist/multi-atlas-loader/bar.d.ts"
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GN-c/phaser3-protobuf-atlas.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GN-c/phaser3-protobuf-atlas/issues"
  },
  "homepage": "https://github.com/GN-c/phaser3-protobuf-atlas#readme",
  "dev-dependencies": {
    "protobufjs-cli": "^1.1.2"
  },
  "peer-dependencies": {
    "phaser": "^3.70.0"
  },
  "dependencies": {
    "phaser": "^3.70.0",
    "protobufjs": "^7.2.5",
    "protobufjs-cli": "^1.1.2",
    "typescript": "^5.3.3"
  }
}
