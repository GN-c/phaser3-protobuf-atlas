{
  "name": "phaser3-protobuf-atlas",
  "version": "1.2.5",
  "description": "",
  "scripts": {
    "build": "npm run generate-proto & tsup",
    "generate-proto": "pbjs src/protobuf/atlas.proto -l -t static-module --no-create --no-encode --no-verify --no-convert --no-delimited --no-service -w src/protobuf/wrapper --es6 -o src/protobuf/index.js",
    "postgenerate-proto": "pbts src/protobuf/index.js -m -o src/protobuf/index.d.ts"
  },
  "tsup": {
    "splitting": true,
    "clean": true,
    "dts": true,
    "sourcemap": true,
    "treeshake": true,
    "format": [
      "cjs",
      "esm"
    ],
    "minify": false,
    "bundle": true,
    "skipNodeModulesBundle": true,
    "target": "es6",
    "outDir": "dist",
    "entry": {
      "encode": "src/encode/index.ts",
      "multi-atlas-loader": "src/multi-atlas-loader/index.ts",
      "atlas-loader": "src/atlas-loader/index.ts"
    }
  },
  "exports": {
    "./*": {
      "import": "./dist/*.mjs",
      "types": "./dist/*.d.ts",
      "require": "./dist/*.js",
      "default": "./dist/*.js"
    }
  },
  "typesVersions": {
    "*": {
      "*": [
        "dist/*.d.ts"
      ]
    }
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/GN-c/phaser3-protobuf-atlas.git"
  },
  "author": "",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/GN-c/phaser3-protobuf-atlas/issues"
  },
  "homepage": "https://github.com/GN-c/phaser3-protobuf-atlas#readme",
  "dev-dependencies": {
    "protobufjs-cli": "^1.1.2"
  },
  "peer-dependencies": {},
  "dependencies": {
    "phaser": ">=3.0.0",
    "protobufjs": "^7.2.5",
    "protobufjs-cli": "^1.1.2",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  },
  "files": [
    "dist/**",
    "README.md"
  ]
}
